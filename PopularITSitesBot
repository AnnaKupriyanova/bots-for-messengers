import telebot
a = telebot.TeleBot("5216515502:AAGyOscUIA0-Fo_He-UL7R3n4Y3n_x-P7FM")
keyboard_test = telebot.types.ReplyKeyboardMarkup(True)
keyboard_test.row("Тестирование", "Веб-дизайн", "Кодирование")
@a.message_handler(commands=['start'])
def startWork(message):
  tid = message.chat.id
  a.send_message(tid, "start work!", reply_markup = keyboard_test)
@a.message_handler(content_types=['text'])
def sendYourMessage(message):
  mid = message.chat.id
  if message.text == "Тестирование":
    a.send_message(mid, "https://habr.com/ru/company/habr_career/blog/517812/")
    a.send_message(mid, "https://ru.wikipedia.org/wiki/%D0%A2%D0%B5%D1%81%D1%82%D0%B8%D1%80%D0%BE%D0%B2%D0%B0%D0%BD%D0%B8%D0%B5_%D0%BF%D1%80%D0%BE%D0%B3%D1%80%D0%B0%D0%BC%D0%BC%D0%BD%D0%BE%D0%B3%D0%BE_%D0%BE%D0%B1%D0%B5%D1%81%D0%BF%D0%B5%D1%87%D0%B5%D0%BD%D0%B8%D1%8F")
    a.send_message(mid, "http://www.protesting.ru/testing/")
  elif message.text =="Веб-дизайн":
    a.send_message(mid, "https://ru.wikipedia.org/wiki/%D0%92%D0%B5%D0%B1-%D0%B4%D0%B8%D0%B7%D0%B0%D0%B9%D0%BD")
    a.send_message(mid, "https://skillbox.ru/media/design/veb_dizayn_s_nulya/")
    a.send_message(mid, "https://habr.com/ru/hub/web_design/")
  elif message.text =="Кодирование":
    a.send_message(mid, "https://ru.wikipedia.org/wiki/%D0%9A%D0%BE%D0%B4%D0%B8%D1%80%D0%BE%D0%B2%D0%B0%D0%BD%D0%B8%D0%B5_(%D0%BF%D1%80%D0%BE%D0%B3%D1%80%D0%B0%D0%BC%D0%BC%D0%B8%D1%80%D0%BE%D0%B2%D0%B0%D0%BD%D0%B8%D0%B5)")
    a.send_message(mid, "https://bestprogrammer.ru/programmirovanie-i-razrabotka/programmirovanie-i-kodirovanie-v-chyom-raznitsa")
    a.send_message(mid, "https://senior.ua/articles/kodirovanie-dlya-nachinayuschih-4-soveta-po-programmirovaniyu")
  else:
    a.send_message(mid, "Ошибка")
a.polling()
